  "bugs": {
    "url": "https://github.com/nemanjan00/event-piped/issues"
  },
  "homepage": "https://github.com/nemanjan00/event-piped#readme",
  "dependencies": {
    "eslint": "^6.8.0",
    "husky": "^4.0.6",
    "implement-js": "^0.0.27"
  },
  "devDependencies": {
    "eslint-plugin-mocha": "^6.2.2",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0"
